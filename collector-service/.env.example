# ============================================================================
# Collector Service - Environment Variables
# ============================================================================
# Copy this file to .env and fill in your actual values

# Mapbox (for geocoding and maps)
MAPBOX_TOKEN=your_mapbox_token_here

# PredictHQ (for events data)
PredictHQ=your_predicthq_api_key_here

# OpenWeatherMap (for weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Geoapify (for traffic data)
GEOAPIFY_API_KEY=your_geoapify_api_key_here

# Calendarific (for holiday data)
CALENDARIFIC_API_KEY=your_calendarific_api_key_here
CALENDARIFIC_COUNTRY=IN
CALENDARIFIC_YEAR=2025

# Supabase (for data storage) - REQUIRED for save_to_supabase feature
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key_here

# ============================================================================
# Notes:
# - SUPABASE_URL: Your Supabase project URL
# - SUPABASE_KEY: Use the service_role key (not anon key) for write access
# - Get keys from: Project Settings > API in Supabase Dashboard
# ============================================================================
